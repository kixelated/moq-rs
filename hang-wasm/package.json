	},
	"devDependencies": {
		"@rspack/cli": "^1.3.5",
		"@rspack/core": "^1.3.5",
		"@wasm-tool/wasm-pack-plugin": "^1.7.0",
		"html-webpack-plugin": "^5.6.3",
		"ts-loader": "^9.5.2",
		"wasm-pack": "^0.13.1"
	},
	"dependencies": {
		"@types/audioworklet": "^0.0.74"
	}
}
